@using EntityLayer.Concrete
@model List<Address>
<div class="w3l_header_left">
	@foreach (var item in Model)
	{
		<ul>
			<li>
				<span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>
				<a href="https://www.google.com/maps/search/@item.Description1,@item.Description2">@item.Description1, @item.Description2</a>
			</li>

			<li><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span> <a href="tel:@item.Description4">@item.Description4</a></li>
			<li><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> <a href="mailto:@item.Description3">@item.Description3</a></li>
		</ul>
	}
	
</div>